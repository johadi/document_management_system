{
  "name": "document_management_system",
  "version": "1.0.0",
  "description": "The Document management system is a system that manages documents with types, users and user roles. Each document defines access rights; the document defines which roles can access it",
  "main": "app.js",
  "scripts": {
    "start": "npm run db_migrate && webpack -p && node node_modules/gulp/bin/gulp",
    "start_dev": "npm run db_migrate && node node_modules/gulp/bin/gulp",
    "db_migrate": "sequelize db:migrate",
    "undo_migrate": "sequelize db:migrate:undo:all",
    "db_seed": "sequelize db:seed:all",
    "undo_seed": "sequelize db:seed:undo:all",
    "test_db_migrate": "NODE_ENV=test sequelize db:migrate",
    "test_undo_migrate": "NODE_ENV=test sequelize db:migrate:undo:all",
    "test": "NODE_ENV=test sequelize db:migrate && NODE_ENV=test nyc --reporter=html --reporter=text mocha server/tests/**/*.spec.js --reporter spec --compilers js:babel-register --slow 5000",
    "test_frontend": "NODE_ENV=test mocha --compilers js:babel-register --require client/tests/components/setUp.js client/tests/components/*.spec.js client/tests/reducers/*.spec.js client/tests/actions/*.spec.js",
    "e2e_test": "node_modules/.bin/nightwatch --config nightwatch.conf.js",
    "e2e_setup": "node_modules/selenium-standalone/bin/selenium-standalone install",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andela-eshaibu/document_management_system.git"
  },
  "keywords": [
    "document",
    "management",
    "system",
    "API",
    "JavaScript"
  ],
  "author": "Shaibu Emmanuel",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andela-eshaibu/document_management_system/issues"
  },
  "homepage": "https://github.com/andela-eshaibu/document_management_system#readme",
